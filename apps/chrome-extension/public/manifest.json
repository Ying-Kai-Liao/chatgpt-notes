{
  "manifest_version": 3,
  "name": "Web Activity Monitor",
  "version": "1.0",
  "description": "Monitor web requests",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
